    <servlet-name>TraceDisplayBeanServlet</servlet-name>
    <url-pattern>/TraceDisplayBeanServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>WatchServlet</servlet-name>
    <servlet-class>com.hawkeye.servlet.WatchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WatchServlet</servlet-name>
    <url-pattern>/WatchServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CheckStatusServlet</servlet-name>
    <servlet-class>com.hawkeye.servlet.ChkStatusServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckStatusServlet</servlet-name>
    <url-pattern>/CheckStatus</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>Cpindex.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>MaintenanceServlet</display-name>
    <servlet-name>MaintenanceServlet</servlet-name>
    <servlet-class>com.hawkeye.servlet.MaintenanceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MaintenanceServlet</servlet-name>
    <url-pattern>/MaintenanceServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>StatsCollectorServlet</display-name>
    <servlet-name>StatsCollectorServlet</servlet-name>
