  <servlet-mapping>
    <servlet-name>MemoryUIServlet</servlet-name>
    <url-pattern>/MemoryUIServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>WSStatsDisplayServlet</display-name>
    <servlet-name>WSStatsDisplayServlet</servlet-name>
    <servlet-class>com.hawkeye.servlet.WSStatsDisplayServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WSStatsDisplayServlet</servlet-name>
    <url-pattern>/WSStatsDisplayServlet</url-pattern>
  </servlet-mapping>
